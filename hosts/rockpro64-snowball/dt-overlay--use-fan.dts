/dts-v1/;
/plugin/;

/ {
  //compatible = "pine64,rockpro64v2.1", "pine64,rockpro64", "rockchip,rk3399";
  compatible = "pine64,rockpro64v2.1", "pine64,rockpro64";

  fragment@0 {
    target-path = "/pwm-fan";
    __overlay__ {
      cooling-min-state = <0>;
      cooling-max-state = <3>;
      //FIXME add 80
      cooling-levels = <0 102 170 230>;
    };
  };

  //FIXME adjust thermal-zones
};
